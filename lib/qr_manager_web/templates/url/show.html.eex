<h1>Show Url</h1>

<ul>

  <li>
    <strong>Url:</strong>
    <%= @url.url %>
  </li>

  <li>
    <strong>Url shortened:</strong>
    <%= @url.url_shortened %>
  </li>

  <li>
    <strong>User ID:</strong>
    <%= @url.user_id %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.user_url_path(@conn, :edit, @url.user_id, @url.id) %></span>
<span><%= link "Back", to: Routes.user_url_path(@conn, :index, @url.user_id) %></span>
